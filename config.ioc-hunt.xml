<?xml version="1.0"?>
<Settings>
 <Global>
	<sqlConnectString>Data Source=tcp:10.1.10.1;Database=IOC-hunt;Integrated Security=false;UID=hash_user;Password=password;</sqlConnectString>
	<textOutputFile>C:\logfile-$hostname.log</textOutputFile>
	<httpOutputUrl>http://webserver</httpOutputUrl>
	<syslogServer>172.16.135.132</syslogServer>
	<syslogPort>10514</syslogPort>
	<syslogFacility>22</syslogFacility>
	<excludeHostnamePattern>*-*-*</excludeHostnamePattern>
	<fileCopySleep>15</fileCopySleep>
	<hostBatchSize>50</hostBatchSize>
	<hostBatchDelay>30</hostBatchDelay>
	<profileServer></profileServer>
	<profilePath></profilePath>
	<homeServer></homeServer>
	<homePath></homePath>
	<remote_basedir>\windows\temp\</remote_basedir>
 </Global>
 <Yara>
	<ruleFile>rules.yar</ruleFile>
 </Yara>
 <Tasks>
	<Autoruns>
		<baseDir>C:\</baseDir>
	</Autoruns>
	<files_hash>
		<filePath>C:\</filePath>
		<maxFileSize>15000000</maxFileSize>
	</files_hash>
 </Tasks>
 <HttpListener>
	<httpPort>8008</httpPort>
 </HttpListener>
</Settings>
